<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0D1B2A">

    <Button
        android:id="@+id/buttonHistorial"
        android:layout_width="131dp"
        android:layout_height="43dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="560dp"
        android:text="@string/btnHistorial"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="96dp"
        android:layout_height="43dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="304dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/jugadorbeige" />

    <EditText
        android:id="@+id/editTextJ3"
        android:layout_width="113dp"
        android:layout_height="52dp"
        android:layout_marginStart="276dp"
        android:layout_marginTop="360dp"
        android:ems="10"
        android:hint="@string/placeHolder3"
        android:inputType="text"
        android:textColorHint="#40FFC266"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="96dp"
        android:layout_height="43dp"
        android:layout_marginStart="284dp"
        android:layout_marginTop="304dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/jugadorbeige" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="96dp"
        android:layout_height="43dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="304dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/jugadorbeige" />

    <EditText
        android:id="@+id/editTextJ2"
        android:layout_width="113dp"
        android:layout_height="52dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="360dp"
        android:ems="10"
        android:hint="@string/placeHolder2"
        android:textColorHint="#40FFC266"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonEntrar"
        android:layout_width="131dp"
        android:layout_height="43dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="472dp"
        android:text="@string/btnComenzar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextJ1"
        android:layout_width="113dp"
        android:layout_height="52dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="360dp"
        android:ems="10"
        android:textColorHint="#40FFC266"
        android:hint="@string/placeHolder1"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="257dp"
        android:layout_height="338dp"
        android:layout_marginStart="77dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />


</androidx.constraintlayout.widget.ConstraintLayout>